{
  "name": "dummy-db",
  "version": "0.1.0",
  "description": "A locally hosted JSON-database for quick prototyping",
  "author": "CasperSocio (https://github.com/CasperSocio)",
  "license": "MIT",
  "type": "module",
  "scripts": {
    "build": "tsup src/index.ts --format esm,cjs --dts --outDir dist --sourcemap"
  },
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      },
      "require": {
        "types": "./dist/index.d.cts",
        "default": "./dist/index.cjs"
      }
    }
  },
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "repository": {
    "directory": "packages/dummy-db",
    "type": "git",
    "url": "git+https://github.com/dummy-db/dummy-db.git"
  },
  "bugs": {
    "url": "https://github.com/dummy-db/dummy-db/issues"
  },
  "devDependencies": {
    "@dummy-db/types": "workspace:*",
    "@types/jest": "catalog:",
    "@types/node": "catalog:",
    "jest": "catalog:",
    "ts-jest": "catalog:",
    "tsup": "^8.5.0",
    "typescript": "catalog:"
  }
}